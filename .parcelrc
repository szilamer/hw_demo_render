{
  "extends": "@parcel/config-default",
  "reporters": ["@parcel/reporter-dev-server"],
  "transformers": {
    "*.{js,mjs,jsx,cjs,ts,tsx}": ["@parcel/transformer-js", "@parcel/transformer-react-refresh-wrap"],
    "*.{css,pcss}": ["@parcel/transformer-css"],
    "*.{pdf,txt,jpg,jpeg,png}": ["@parcel/transformer-raw"]
  },
  "resolvers": ["@parcel/resolver-default"],
  "namers": ["@parcel/namer-default"],
  "packagers": {
    "*.{pdf,txt,jpg,jpeg,png}": "@parcel/packager-raw"
  },
  "optimizers": {
    "*.{pdf,txt,jpg,jpeg,png}": []
  }
} 